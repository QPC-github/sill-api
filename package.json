{
    "name": "sillfr",
    "version": "0.0.5",
    "description": "Ongoing work with the SILL 2.0",
    "repository": {
        "type": "git",
        "url": "git://github.com/etalab/sill.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "generate-json-from-csv": "node dist/bin/generateJsonFromCsv.js",
        "build": "tsc",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "bin": {
        "generate-easily-consumable-entries": "dist/bin/generate-easily-consumable-entries.js"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "DINUM",
    "license": "etalab-2.0",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/etalab/sill",
    "devDependencies": {
        "@types/node": "^16.4.9",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "csv-parse": "^5.0.4",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^2.3.2",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "evt": "^2.0.0-beta.39",
        "tsafe": "^0.9.0"
    }
}
