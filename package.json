{
    "name": "sill-api",
    "version": "0.28.3",
    "description": "The backend of sill.etalab.gouv.fr",
    "repository": {
        "type": "git",
        "url": "git://github.com/etalab/sill-web.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc && yarn grant-exec-perms",
        "dev": "./.env.local.sh node dist/server/main.js",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "grant-exec-perms": "node dist/bin/tools/grant-exec-perms.js"
    },
    "bin": {
        "build-data": "dist/bin/build-data.js"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "DINUM",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/etalab/sill",
    "devDependencies": {
        "@octokit/rest": "^18.12.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "4.17.13",
        "@types/memoizee": "^0.4.7",
        "@types/node": "^16.4.9",
        "@types/node-fetch": "^2.5.7",
        "@types/ungap__structured-clone": "^0.3.0",
        "comment-json": "^3.0.0",
        "cors": "^2.8.5",
        "cra-envs": "^1.2.1",
        "csv-parse": "^5.0.4",
        "express": "^4.17.2",
        "husky": "^4.3.8",
        "keycloak-backend": "^2.0.1",
        "keycloakify": "^6.8.4",
        "lint-staged": "^11.1.1",
        "prettier": "^2.7.1",
        "typescript": "^4.9.1-beta",
        "url-join": "^4.0.1",
        "deepmerge": "^4.2.2",
        "@types/deepmerge": "^2.2.0"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.8.6",
        "@trpc/server": "^9.24.0",
        "@ungap/structured-clone": "^0.3.4",
        "async-mutex": "^0.4.0",
        "cheerio": "^1.0.0-rc.5",
        "compression": "^1.7.4",
        "evt": "^2.4.10",
        "i18nifty": "^1.3.26",
        "jwt-simple": "^0.5.6",
        "memoizee": "^0.4.15",
        "node-fetch": "^2.6.7",
        "redux-clean-architecture": "^2.5.1",
        "run-exclusive": "^2.2.16",
        "tsafe": "^1.1.3",
        "zod": "^3.17.3"
    }
}
