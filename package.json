{
    "name": "sill-api",
    "version": "0.4.0",
    "description": "Ongoing work with the SILL 2.0",
    "repository": {
        "type": "git",
        "url": "git://github.com/etalab/sill-web.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "dev": "./.env.local.sh node dist/server/main.js",
        "build-data": "node dist/bin/build-data.js etalab/sill-data incremental=true",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v",
            "prepare-commit-msg": "exec < /dev/tty && cz --hook || true"
        }
    },
    "config": {
        "commitizen": {
            "path": "cz-emoji"
        }
    },
    "author": "DINUM",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/etalab/sill",
    "devDependencies": {
        "@octokit/rest": "^18.12.0",
        "@types/cors": "^2.8.12",
        "@types/express": "4.17.13",
        "@types/memoizee": "^0.4.7",
        "@types/node": "^16.4.9",
        "@types/node-fetch": "^2.5.7",
        "@types/ungap__structured-clone": "^0.3.0",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "@ungap/structured-clone": "^0.3.4",
        "cheerio": "^1.0.0-rc.5",
        "comment-json": "^3.0.0",
        "commitizen": "^4.2.4",
        "cors": "^2.8.5",
        "cra-envs": "^1.2.1",
        "csv-parse": "^5.0.4",
        "csv-stringify": "^6.0.5",
        "cz-emoji": "^1.3.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "evt": "^2.0.0-beta.39",
        "express": "^4.17.2",
        "husky": "^4.3.8",
        "keycloak-backend": "^2.0.1",
        "lint-staged": "^11.1.1",
        "memoizee": "^0.4.15",
        "node-fetch": "^2.6.7",
        "prettier": "^2.3.2",
        "run-exclusive": "^2.2.14",
        "typescript": "^4.6.2",
        "url-join": "^4.0.1"
    },
    "dependencies": {
        "@trpc/server": "^9.20.3",
        "jwt-simple": "^0.5.6",
        "tsafe": "^0.9.0",
        "zod": "^3.11.6"
    }
}
